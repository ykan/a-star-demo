{
  "name": "a-star-demo",
  "version": "1.0.0",
  "description": "a-star-demo",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "commit": "git add . && cz",
    "clean": "rimraf coverage",
    "test": "BABEL_ENV=test jest --detectOpenHandles --forceExit --verbose --coverage",
    "test:last": "BABEL_ENV=test jest --detectOpenHandles --forceExit --verbose --coverage --changedSince origin/main",
    "test:clear": "BABEL_ENV=test jest --clearCache",
    "test:debug": "BABEL_ENV=test node --inspect-brk node_modules/.bin/jest --runInBand packages/react-img/src/useImg.test.tsx",
    "test:watch": "BABEL_ENV=test jest --watch --verbose --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ykan/a-star-demo.git"
  },
  "keywords": [
    "lazyload",
    "img"
  ],
  "author": "ykan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ykan/a-star-demo/issues"
  },
  "homepage": "https://github.com/ykan/a-star-demo#readme",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "@vitejs/plugin-react-refresh": "^1.3.5",
    "babel-jest": "^27.0.6",
    "babel-plugin-module-resolver": "^4.1.0",
    "commitizen": "^4.2.4",
    "eslint": "^7.29.0",
    "eslint-config-alloy": "^4.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.1",
    "prettier": "2.3.2",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.4",
